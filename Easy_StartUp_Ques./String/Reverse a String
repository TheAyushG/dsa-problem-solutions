class Solution {
  public:
    string reverseString(string& s) {
        // code here
        int n = s.length();
        
        int i = 0;
        int j = n-1;
        
        while(i<j){
            char temp = s[i];
            s[i] = s[j];
            s[j] = temp;
            j--;
            i++;
        }
        return s;
    }
};
