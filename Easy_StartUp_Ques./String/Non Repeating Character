
class Solution {
  public:
    char nonRepeatingChar(string &s) {
        //  code here
        
        int n = s.size();
        
        int freq[256] = {0};
        
        for(char ch : s){
            freq[ch]++;
        }
        
        for(char ch : s){
            if(freq[ch] == 1)
            return ch;
        }
        return '$';
    }
};
