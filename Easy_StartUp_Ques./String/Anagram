class Solution {
  publicblic:
    bool areAnagrams(string& s1, string& s2) {
        // code here
        
        int freq[256] = {0};
        
        for(char ch : s1){
            freq[ch]++;
        }
        
        for(char ch : s2){
            freq[ch]--;
        }
        
        for(int i=0; i<256; i++){
            if(freq[i] != 0)
            return false;
        }
        return true;
    }
};
