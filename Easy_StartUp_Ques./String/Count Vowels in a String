class Solution {
public:
    int countVowels(string s) {
        int cnt = 0;

        for (char ch : s) {
            if (ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' ||
                ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U') {
                cnt++;
            }
        }

        return cnt;
    }
};
