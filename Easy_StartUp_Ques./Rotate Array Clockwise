class Solution {
  public:
    void rotateclockwise(vector<int>& arr, int k) {
        // code here
        
        int n = arr.size();
        
        k = k % n;
        
        vector<int>array(n);
        
        for(int i=0; i<n; i++){
            array[(i+k) % n] = arr[i]; 
        }
        
        for(int i=0; i<n; i++){
          arr[i] = array[i];
        }
        
    }
};
