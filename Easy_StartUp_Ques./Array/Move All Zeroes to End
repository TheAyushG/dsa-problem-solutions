class Solution {
  public:
    void pushZerosToEnd(vector<int>& arr) {
        // code here
        
        int n = arr.size();
        int i=0;
        int j=0;
        
        while(i < n){
            if(arr[i] != 0){
                if(i != j){
                    int temp = arr[i];
                    arr[i] = arr[j];
                    arr[j] = temp;
                }
                j++;
            }
            i++;
        }
    }
};
