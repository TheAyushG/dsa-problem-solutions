class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        
        int n = nums.size();

        int count1 = 0;
        int mje1 = NULL;

        int count2 = 0;
        int mje2 = NULL;

        for(int i=0; i<n; i++){
            if(nums[i] == mje1){
                count1++;
            }

            else if(nums[i] == mje2){
                count2++;
            }

            else if(count1 == 0){
                mje1 = nums[i];
                count1 = 1;
            }

            else if(count2 == 0){
                mje2 = nums[i];
                count2 = 1;
            }

            else {
                count1--;
                count2--;
            }
        }
        

        vector<int> result;
        int freq1 = 0;
        int freq2 = 0;

        for(int num : nums){
            if(num == mje1){
            freq1++;
            }

            else if(num == mje2){
                freq2++;
            }
        }

        if(freq1 > n/3){
            result.push_back(mje1);
        }
        if(freq2 > n/3){
            result.push_back(mje2);
        }

        return result;
    }
};
