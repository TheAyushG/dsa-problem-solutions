class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        
        int n = nums.size();
        sort(nums.begin(), nums.end());
        
        if(n <= 1){
            return false;
        }

        for(int i=1; i<n; i++){
            if(nums[i] == nums[i-1]){
               return true;
            }
        }
        return false;
    }
};



